# Local, HuggingFace Local, HuggingFace Inference or OpenAI
MODEL_SOURCE=local
# For local model: Change this to the path of your model (in GGUF format)
# I used https://www.kaggle.com/models/google/gemma/gguf/1.1-2b-it
LLM_MODEL_PATH=models/gemma-1.1-2b-it.gguf
# For HuggingFace model: Change this to the model ID on HuggingFace Hub
HF_ID=openai-community/gpt2
# Put your HuggingFace or OpenAI API token here
API_TOKEN=None
# Change this to True if using demo mock LLM and False if using real LLM
IS_DEMO=True
# DMAS endpoint
VITE_DMAS_ENDPOINT=http://localhost:8081
